{
  "name": "sudokuapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npx tailwindcss -i ./input.css -o ./output.css --watch\"",
    "build": "concurrently \"vite build\" \"npx tailwindcss -i ./input.css -o ./output.css --minify\"",
    "preview": "vite preview"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.10",
    "vite": "^5.3.4"
  }
}
